                                    <p ng-cloak>&nbsp;&nbsp;{{dashboardData.TodaySale.Today_Sales_CR=== "" || dashboardData.TodaySale.Today_Sales_CR===null ? "0": dashboardData.TodaySale.Today_Sales_CR}}</p>
                                </div>
                            </div>
                        </div>
