@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<script src="~/Areas/FSM/NgCode/frmDashboardNationalPrescription.js"></script>

<div ng-app="myApp" ng-controller="myCtrl">
    <div class="loader ng-cloak LoaderStyle" data-loading>

        <img src="~/Content/img/loader.gif" width="100px;" height="100px;" style="top: 50%; left: 50%;" />
    </div>

    <div class="form-horizontal" ng-form="frmDashboardNationalPrescription">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: center; background-color: #800080;color:white;font-weight:bold"><h2>&nbsp National Prescription Dashboard</h2></div>
                </div>
            </div>

        </section>
        <section class="content">
            <!-- Small boxes (Stat box) -->
            <!-- 1st row  -->

            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: left; background-color: #800080;color:white;font-weight:bold"><h4>&nbsp Prescription Summary National </h4></div>
                </div>
            </div>
            </br>
            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-red">
                        <div class="inner">
                            <p>MPO Target</p>
                            <h3 ng-cloak>{{dashboardData.MPOTarget}}</h3>                      
                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <p>  Today Prescription  </p>  
                            <h3 ng-cloak>{{dashboardData.TodayPrescription}}</h3>                    
                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <p> Cumulative Total Prescription  </p>
                            <h3 ng-cloak>{{dashboardData.Cumulative}}</h3> 
                           

                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <p> Achievement </p>
                            <h3 ng-cloak>{{dashboardData.Achievement}}%</h3> 


                         
                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>


            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">

                            <p> Last MPSD </p>
                            <h3 ng-cloak>{{dashboardData.LastMPSD}}</h3> 
                         
                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <p> Last Month </p>
                            <h3 ng-cloak>{{dashboardData.LastMonth}}</h3> 
                           
                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">                   
                            <p> Growth </p>
                            <h3 ng-cloak>{{dashboardData.Growth}}</h3>
                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <p> Growth Percentage </p>
                            <h3 ng-cloak>{{dashboardData.GrowthPercentage}}%</h3>
                           
                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>

            </br>

            </br>
          


            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: left; background-color: #800080;color:white;font-weight:bold"><h4>&nbsp Prescription Summary Monthly  </h4></div>
                </div>
            </div>
            </br>
            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-red-active">
                        <div class="inner">
                            <p> Total MPO </p>
                            <h3 ng-cloak>{{dashboardData.TotalMPO}}</h3>                          
                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">
                            <p> Cumulative Sender MPO </p>
                            <h3 ng-cloak>{{dashboardData.CumulativeSenderMPO}}</h3>   
        
                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green-gradient">
                        <div class="inner">
                            <p> Last MPSD Sender MPO </p>
                            <h3 ng-cloak>{{dashboardData.LastMPSDSenderMPO}}</h3>  
                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <p>   No of Who did'nt Send  </p>
                            <h3 ng-cloak>{{dashboardData.NoOfWhoDidntSend}}</h3>  

                          
                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>
            </br>
            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: left; background-color: #800080;color:white;font-weight:bold"><h4>&nbsp Region Wise Prescription Summary (Current Month) </h4></div>
                </div>
            </div>
            </br>
            <div class="row">
                <div class="col-lg-12 col-xs-12">
                
                        <div id="NationalPrescriptionGrid" ui-grid="NationalPrescriptionGridValue" class="grid" style="height: 320px; width: 100%;" ui-grid-auto-resize ui-grid-resize-columns ui-grid-grouping ui-grid-exporter></div>

                  
                </div>

            </div>

        </section>


    </div>
</div>
