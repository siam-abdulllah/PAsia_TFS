@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<script src="~/Areas/FSM/NgCode/frmDashboardRegionalPrescription.js"></script>

<div ng-app="myApp" ng-controller="myCtrl">
    <div class="loader ng-cloak LoaderStyle" data-loading>

        <img src="~/Content/img/loader.gif" width="100px;" height="100px;" style="top: 50%; left: 50%;" />
    </div>

    <div class="form-horizontal" ng-form="frmDashboardRegionalPrescription">
        <!-- Content Header (Page header) -->
        <section class="content-header">
            <div class="row">

                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: center; background-color: #800080; color: white; font-weight: bold">
                        <h2> Prescription Dashboard </h2>
                    </div>
                </div>
            </div>

        </section>
        <section class="content">
            <!-- Small boxes (Stat box) -->
           
            <div class="row">
                <div class="col-md-2" style="text-align: center">
                    <label class="control-label">Product Type</label>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <ui-select class="form-control form-white" ng-model="frmDashboardRegionalPrescription.ProdType" ng-change="GetDashboardData()" theme="select2" ng-cloak>
                            <ui-select-match placeholder="">{{$select.selected.TYPE_NAME}}</ui-select-match>
                            <ui-select-choices repeat="prodType in ProdTypes | propsFilter: {TYPE_NAME: $select.search, TYPE: $select.search}">
                                <div ng-bind-html="prodType.TYPE_NAME | highlight: $select.search"></div>
                            </ui-select-choices>
                        </ui-select>
                    </div>
                </div>
            </div>
            <!-- 1st row  -->
            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: left; background-color: #800080;color:white;font-weight:bold"><h4>&nbsp {{prodName}} Prescription Summary National </h4></div>
                </div>
            </div>

            <br />
            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-red">
                        <div class="inner">
                            <p>{{prodName}} Commitment</p>
                            <h3>{{dashboardData.Commitment}}</h3>

                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">
                            <p>Today Prescription</p>
                            <h3>{{dashboardData.TodayPrescription}}</h3>

                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">

                            <p>Cumulative</p>
                            <h3>{{dashboardData.Cumulative}}</h3>

                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">

                            <p>Achievement</p>
                            <h3>{{dashboardData.Achievement}}%</h3>



                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>


            <div class="row">
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-yellow">
                        <div class="inner">

                            <p>Last MPSD</p>
                            <h3>{{dashboardData.LastMPSD}}</h3>


                        </div>
                        <div class="icon">
                            <i class="ion ion-bag"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-aqua">
                        <div class="inner">

                            <p>Last Month</p>
                            <h3>{{dashboardData.LastMonth}}</h3>

                        </div>
                        <div class="icon">
                            <i class="ion ion-stats-bars"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">
                            <p>Growth</p>
                            <h3>{{dashboardData.Growth}}</h3>



                        </div>
                        <div class="icon">
                            <i class="ion ion-person-add"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
                <div class="col-lg-3 col-xs-6">
                    <!-- small box -->
                    <div class="small-box bg-green">
                        <div class="inner">

                            <p>Growth</p>
                            <h3>{{dashboardData.GrowthPercentage}} %</h3>



                        </div>
                        <div class="icon">
                            <i class="ion ion-pie-graph"></i>
                        </div>
                        <a href="#" class="small-box-footer">More info <i class="fa fa-arrow-circle-right"></i></a>
                    </div>
                </div>
                <!-- ./col -->
            </div>

            <br/>

            <div class="row">
                <div class="col-lg-12 col-xs-12">
                    <div class="row" style="width: 100%; text-align: left; background-color: #800080;color:white;font-weight:bold"><h4>&nbsp Region  {{prodName}} Wise Prescription Summary </h4></div>
                </div>
            </div>
            <br/>
            <div class="row">
                <div class="col-lg-12 col-xs-12">

                    <div id="RegionalPrescriptionGrid" ui-grid="RegionalPrescriptionGridValue" class="grid" style="height: 320px; width: 100%;" ui-grid-auto-resize ui-grid-resize-columns ui-grid-grouping ui-grid-exporter></div>


                </div>
            </div>


        </section>


    </div>
</div>
